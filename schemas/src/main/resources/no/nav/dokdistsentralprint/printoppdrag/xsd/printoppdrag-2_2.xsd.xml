    <!-- DokumentInfo attributter: destinasjon = "INNLAND", "EUROPA" eller "VERDEN". sorteringsfelt = "USORTERT" -->
    <DokumentInfo destinasjon="INNLAND" sorteringsfelt="USORTERT"/>

    <!-- Kanal attributter:
		behandling = "[PRIO]_[KONVTYPE]_[PLEX]",
			[PRIO] = A eller B,
			[KONVTYPE] = X (pakkes i konvolutt med vindu) eller W (pakkes i konvolutt uten vindu),
			[PLEX] = S (simplex) eller D (duplex).
		type = "PRINT" -->
    <Kanal behandling="A_X_D" type="PRINT"/>

  </BestillingsInfo>

